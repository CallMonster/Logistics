<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:background="@color/drak_transparent">
    <LinearLayout android:background="@color/grey_low"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <include layout="@layout/nav_layout"
            android:id="@+id/contract_us_nav"/>
        <RelativeLayout android:id="@+id/contract_us_call_layout"
            android:layout_width="match_parent"
            android:layout_height="44dp"
            android:background="@color/colorWhite">
            <ImageView android:id="@+id/label7"
                android:layout_width="16dp"
                android:layout_height="16dp"
                android:src="@mipmap/navi_call_selected"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/activity_horizontal_margin"/>
            <TextView android:text="政通电话"
                android:textColor="@color/grey_normal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/label7"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/space_8"/>
            <ImageView android:id="@+id/label8"
                android:layout_width="16dp"
                android:layout_height="16dp"
                android:src="@mipmap/derection_icon"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/activity_horizontal_margin"
                android:layout_centerVertical="true"/>
            <TextView android:layout_toLeftOf="@id/label8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="022-00099000"
                android:textColor="@color/tab_seleted"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/space_8"
                android:textSize="@dimen/font_12"/>
        </RelativeLayout>
        <RelativeLayout android:id="@+id/contract_us_email_layout"
            android:layout_width="match_parent"
            android:layout_height="44dp"
            android:background="@color/colorWhite"
            android:layout_marginTop="1dp">
            <ImageView android:id="@+id/label9"
                android:layout_width="16dp"
                android:layout_height="16dp"
                android:src="@mipmap/email_icon"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/activity_horizontal_margin"/>
            <TextView android:text="电子邮箱"
                android:textColor="@color/grey_normal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/label9"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/space_8"/>
            <ImageView android:id="@+id/label10"
                android:layout_width="16dp"
                android:layout_height="16dp"
                android:src="@mipmap/derection_icon"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/activity_horizontal_margin"
                android:layout_centerVertical="true"/>
            <TextView android:layout_toLeftOf="@id/label10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ZTLogistics@163.com"
                android:textColor="@color/tab_seleted"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/space_8"
                android:textSize="@dimen/font_12"/>
        </RelativeLayout>

        <RelativeLayout android:id="@+id/accc_layout"
            android:layout_width="match_parent"
            android:layout_height="56dp"
            android:background="@color/colorWhite"
            android:layout_marginTop="8dp">
            <ImageView android:id="@+id/label11"
                android:layout_width="16dp"
                android:layout_height="16dp"
                android:src="@mipmap/list_location"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/activity_horizontal_margin"/>
            <TextView android:text="公司地址"
                android:textColor="@color/grey_normal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/label11"
                android:layout_marginTop="@dimen/space_10"
                android:layout_marginLeft="@dimen/space_8"
                android:id="@+id/label12"/>

            <TextView android:layout_toRightOf="@id/label11"
                android:layout_below="@id/label12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ZTLogistics@163.com"
                android:textColor="@color/home_grey"
                android:id="@+id/contract_us_address_text"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/space_8"
                android:layout_marginTop="@dimen/space_5"
                android:textSize="@dimen/font_12"/>
        </RelativeLayout>
    </LinearLayout>

</LinearLayout>